    "nuxt-seo-kit": "^0.4.3",
    "onchange": "^7.1.0",
    "pnpm": "^7.19.0",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "sass": "^1.57.1",
    "theme-change": "^2.2.0",
    "typescript": "^4.9.4"
  }
